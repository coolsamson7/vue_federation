{
  "name": "user-module",
  "root": "apps/user-module",
  "sourceRoot": "apps/user-module/src",
  "projectType": "library",
  "targets": {
    "scan": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run metadata:scan --args='--moduleFolder=apps/user-module --outFile=dist/apps/user-module/assets/metadata.json'"
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["scan"],
      "options": {
        "viteConfig": "apps/user-module/vite.config.ts",
        "outputPath": "dist/apps/user-module",
        "emptyOutDir": true
      },
      "configurations": {
        "lib": {
          "env": {
            "BUILD_MODE": "lib"
          }
        }
      }
    },

    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "user-module:build:lib",
        "port": 5003
      }
    },

    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "user-module:build:lib",
        "port": 5003
      }
    }
  },
  "tags": []
}
